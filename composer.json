{
  "name": "gordonmcvey/warp-core-example-app",
  "description": "Collection of example skeleton apps for the Warp Core microframework",
  "minimum-stability": "stable",
  "license": "apache",
  "authors": [
    {
      "name": "Gordon McVey",
      "email": "email@example.com"
    }
  ],
  "require": {
    "php": ">=8.3.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.13",
    "phpstan/phpstan": "^2.1",
    "overtrue/phplint": "^9.6"
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs",
    "lint": "vendor/bin/phplint -c ./phplint.yaml.dist",
    "static": "vendor/bin/phpstan",
    "all": [
      "@phpcs",
      "@lint",
      "@static"
    ]
  }
}
